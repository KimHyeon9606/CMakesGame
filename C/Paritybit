	{
	a[i] = rand () % 1 + 1;
	
	}	
	
	
	
	
	
	for(i=1;i<=8;i++)
	{
		if(a[i] % 2 == 0)
		b++
		
				
	}
	
	if(b % 2 == 0)
	 pa++;
	 


