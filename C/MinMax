#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main(int argc, char *argv[]) {
	int Max,i,Min;

int d[10] = {};
for( i=0; i<10; i++)
{
	printf("정수값입력 :  ");
	scanf("%d",&d[i]);
}

Min = d[0];
Max = d[0];
	
	for (i=1;i<10; i++)
	{
	
	
			if(d[i] > Max)
				Max = d[i];
			if(d[i] < Min)
				Min = d[i];		
		
		
		}
		
		
		printf_s("10개중 최대값은 %d 입니다 \n",Max);
		printf_s("10개 중 최소값은 %d 입니다\n",Min);
		
	
	
	
	return 0;
}
